{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "esmodules": true
                },
            }
        ],
        ["@babel/typescript"]
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", {
            "loose": true,
            "spec": true
        }]
    ]
}
