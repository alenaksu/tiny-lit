{
    "compilerOptions": {
        "target": "es2015",
        "module": "es2015",
        "lib": ["es2017", "esnext.asynciterable", "dom"],
        // "declaration": true,
        "sourceMap": true,
        "inlineSources": true,
        // "outDir": "./",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true,
        "allowJs": true
    }
}
